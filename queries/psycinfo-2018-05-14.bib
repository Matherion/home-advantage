ISSN = {0264-0414},
Journal = {Journal of Sports Sciences},
Keywords = {home advantage, coaches, expectations, goal setting, tactical decisions, Adult, Athletic Performance, Competitive Behavior, Decision Making, Female, Goals, Humans, Male, Middle Aged, Soccer, Athletic Performance, Coaches, Decision Making, Goal Setting, Soccer},
