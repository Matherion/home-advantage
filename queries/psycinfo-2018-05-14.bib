ISSN = {0895-2779},
Journal = {Journal of Sport & Exercise Psychology},
Keywords = {crowd noise, cues, referee decisions, home advantage, soccer, athletic performance, sport psychology, Cues, Decision Making, Germany, Humans, Judgment, Learning, Male, Noise, Population Density, Soccer, Sociology, Sports, Track and Field, Athletic Performance, Cues, Noise Effects, Soccer, Sport Psychology},
