@screening{Whitehead2015-20180523,
  author = {Whitehead, George and Smith, Stephanie H. and Kitzrow, Andrew P. and Wanek, Heather M. and Cuadra, Jessica R.},
  title = {Reducing the home advantage: The example of men's World Cup soccer.},
  year = {2015/06//},
  abstract = {This study examined the home field advantage in men’s World Cup soccer. A framework explaining a portion of this advantage includes game location factors, such as rule changes. To examine whether changes in rules may alter the home advantage, we focused on the change made to the rules for the men’s World Cup. In 1982, for the first time the Fédération Internationale de Football Association (FIFA) increased the number of qualifying teams from 16 to 24 and in 1998 from 24 to 32, thereby decreasing the probability of a home team advantage by chance alone. We predicted and found that prior to 1982 more teams playing in their home country made the finals (63.6%) than from 1982 to 2014 (11.1%). The former was significantly different from what would be expected by chance (12.5%), whereas the latter was not significantly different from what would be expected by chance (6.25%). However, travel might also account for this difference. Implications are discussed. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
