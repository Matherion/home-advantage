  database.provider = {EBSCOhost},
  database.name = {psyh},
  ER = {},
  journal.abbreviated = {J Sport Exerc Psychol},
  U1 = {Sponsor: Deutsche Forschungsgemeinschaft, Germany. Grant: UN 273/1-1. Recipients: Unkelbach, Christian},
