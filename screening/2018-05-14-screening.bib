@screening{Unkelbach2010-20180514,
  type = {article},
  id.record = {2010-24538-003},
  number.accession = {2010-24538-003},
  author = {Unkelbach, Christian and Memmert, Daniel},
  title = {Crowd noise as a cue in referee decisions contributes to the home advantage.},
  journal = {Journal of Sport & Exercise Psychology},
  JO = {Journal of Sport & Exercise Psychology},
  year = {2010/08//},
  volume = {32},
  issue = {4},
  page.start = {483},
  page.end = {498},
  place.published = {US},
  publisher = {Human Kinetics},
  number.serial = {0895-2779, 1543-2904},
  notes = {Accession Number: 2010-24538-003. PMID: 20733209 Other Journal Title: Journal of Sport Psychology. Partial author list: First Author & Affiliation: Unkelbach, Christian; University of Heidelberg, Heidelberg, Germany. Release Date: 20101213. Publication Type: Journal (0100), Peer Reviewed Journal (0110). Format Covered: Electronic. Document Type: Journal Article. Language: English. Grant Information: Unkelbach, Christian. Major Descriptor: Athletic Performance; Cues; Noise Effects; Soccer; Sport Psychology. Classification: Sports (3720). Population: Human (10). Location: Germany. Age Group: Adulthood (18 yrs & older) (300); Young Adulthood (18-29 yrs) (320). Methodology: Empirical Study; Quantitative Study. References Available: Y. Page Count: 16. Issue Publication Date: Aug, 2010. Publication History: Revised Date: Apr 1, 2010; First Submitted Date: Oct 5, 2009. Copyright Statement: Human Kinetics, Inc. 2010.},
  abstract = {The home advantage is one of the best established phenomena in sports, and crowd noise has been suggested as one of its determinants. However, the psychological processes that mediate crowd noise influence and its contribution to the home advantage are still unclear. We propose that crowd noise correlates with the criteria referees have to judge. As crowd noise is a valid cue, referee decisions are strongly influenced by crowd noise. Yet, when audiences are not impartial, a home advantage arises. Using soccer as an exemplar, we show the relevance of this influence in predicting outcomes of real games via a database analysis. Then we experimentally demonstrate the influence of crowd noise on referees’ yellow cards decisions in soccer. Finally, we discuss why the focus on referee decisions is useful, and how more experimental research could benefit investigations of the home advantage. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {crowd noise, cues, referee decisions, home advantage, soccer, athletic performance, sport psychology, Cues, Decision Making, Germany, Humans, Judgment, Learning, Male, Noise, Population Density, Soccer, Sociology, Sports, Track and Field, Athletic Performance, Cues, Noise Effects, Soccer, Sport Psychology},
